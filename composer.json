{
    "name": "angelxmoreno/dockerify",
    "description": "Simple Local Dev Docker containers",
    "require": {
        "cakephp/filesystem": "^3.5",
        "vlucas/phpdotenv": "^2.4",
        "symfony/console": "^3.4",
        "cakephp/collection": "^3.5",
        "rakshazi/get-set-trait": "^1.0",
        "cakephp/utility": "^3.5",
        "symfony/yaml": "^3.4"
